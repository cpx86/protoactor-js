{
  "compilerOptions": {
    "lib": [
      "es2015"
    ],
    "types": [
      "node"
    ],
    "outDir": "dist",
    "declaration": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "removeComments": true,
    "noUnusedLocals": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "inlineSources": true,
    "target": "es6"
  }
}